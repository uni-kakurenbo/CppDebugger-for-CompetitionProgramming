#include "template/standard.hpp"

signed main() {
    int n; cin >> n;
    valarray<i64> a(n); input >> a;
    string s; cin >> s;

    int m; cin >> m;
    debug(uni::ljust(a, m));
    debug(uni::cjust(a, m));
    debug(uni::rjust(a, m));

    debug(uni::ljust(s, m, ' '));
    debug(uni::cjust(s, m, ' '));
    debug(uni::rjust(s, m, ' '));
}
