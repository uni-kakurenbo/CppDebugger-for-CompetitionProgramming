OPTIONS=(
    "-std=gnu++20"
    "-Wno-misleading-indentation"
    "-Wall"
    "-Wextra"
    "-fconcepts-diagnostics-depth=5"
    "-Wno-char-subscripts"
    "-fsplit-stack"
    "-fstack-protector-all"
    "-Wconversion"
    "-Wfloat-equal"
    "-Wshadow"
    "-Wformat=2"
    "-Wlogical-op"
    "-Wshift-overflow=2"
    "-Wduplicated-cond"
    "-D_FORTIFY_SOURCE=2"
    "-fsanitize=undefined,leak,address"
    "-O2"
)
